<form class="ER" method="post">
    <fieldset>
        <legend>建立企业间关系</legend>
        <div class="control-group">
            <label class="control-label" for="sendAdmin">本方企业ID</label>
            <div class="controls">
                <input type="text" class="input-xlarge" id="sendAdmin" name="sendAdmin">
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="toAdmin">其他企业ID</label>
            <div class="controls">
                <input type="text" class="input-xlarge" id="toAdmin" name="toAdmin">
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="status">关系状态</label>
            <div class="controls">
                <input type="text" class="input-xlarge" id="status" name="status">
            </div>
        </div>
        <div class="form-actionsC">
            <button type="submit" class="btn btn-primary">建立关系</button>
        </div>
    </fieldset>
</form>
